<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="app.mirea.ru.rtuapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ТЫЖПРОГРАММИСТ"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        android:name="app.mirea.ru.rtuapp.App">

        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme"
            android:label="">
            <intent-filter>
                <action android:name="app.mirea.ru.rtuapp.MainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>

        <activity
            android:name=".GitLogin"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity
            android:name=".DeviceInfo" >
            <intent-filter>
                <action android:name="app.mirea.ru.rtuapp.DeviceInfo" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ApplicationMaps">
            <intent-filter>
                <action android:name="app.mirea.ru.rtuapp.ApplicationMaps" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>

        <activity
            android:name=".MapsActivity">
            <intent-filter>
                <action android:name="app.mirea.ru.rtuapp.MapsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>